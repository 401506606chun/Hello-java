package cn.itcast.day07.demo02;

public class Person {
	private String name;
	private int age;
	
	public Person(){
		
	}
	public Person(String name,int age){
		this.name = name;
		this.age = age;
		
	}
	public String getName(){
		return name;
	}
	public void setName(Sting name){
		this.name = name;
	}

}
