package practiceCode;

import java.util.Scanner;

/*
 * 7、java从键盘输入三个整数,实现从小到大排序
 * 
 * 分析：
 * 输入三个int a b c
 * 比较大小
*/
public class Demo07 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("请输入三个数：");
		
		int a = sc.nextInt();
		int b = sc.nextInt();
		int c = sc.nextInt();
		
		System.out.println("输入的三个数为：a="+a+"，b"+"="+b+"，c"+"="+c);
		
		
	}

}
